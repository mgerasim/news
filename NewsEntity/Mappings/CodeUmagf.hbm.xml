<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="NewsEntity.Models" assembly="NewsEntity">
  <class name="CodeUmagf" table="Geospace_CodeUmagf">
    <id name="ID" unsaved-value="0">
      <column name="ID" not-null="true" />
      <generator class="identity"/>
    </id>
    <property name="created_at" />
    <property name="updated_at" />

    <property name="HH" >
      <column name="HH" check="(HH &gt;= 0 AND HH &lt;= 23)" unique-key="date" not-null="true" />
    </property>

    <property name="MM">
      <column name="MM" check="(MM &gt;= 1 AND MM &lt;= 12)" unique-key="date" not-null="true"  />
    </property>

    <property name="DD">
      <column name="DD" check="(DD &gt;= 1 AND DD &lt;= 31)" unique-key="date" not-null="true"  />
    </property>

    <property name="YYYY" not-null="true" unique-key="date" />

    <property name="MI" unique-key="date" not-null="true" />


    <property name="Station_ID" />

    <property name="k1" />
    <property name="k2" />
    <property name="k3" />
    <property name="k4" />
    <property name="k5" />
    <property name="k6" />
    <property name="k7" />
    <property name="k8" />

    <property name="ak" />


    <property name="events" type="string" length="100" />

    <property name="Raw" type="string" length="10000"/>
    <property name="ErrorMessage" type="string" length="10000"/>

  </class>
</hibernate-mapping>